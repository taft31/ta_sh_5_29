<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.iotek.model.UsersEntity" table="USERS" schema="SCOTT">
        <id name="id">
            <column name="ID" sql-type="char(32)" length="32"/>
            <generator class="assigned"></generator>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar2(15)" length="15" not-null="true"/>
        </property>
        <property name="pass">
            <column name="PASS" sql-type="char(32)" length="32" not-null="true"/>
        </property>
        <property name="img">
            <column name="IMG" sql-type="varchar2(32)" length="32" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>